@model List<DojoArtesMarciais.Models.TipoDeAssociacao>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h2>TipoDeAssociacao</h2>

@if (Model.Count > 0)
{
    <table class="table table-bordered table-hover">
        <thead>
            <tr><th>TipoDeAssociacao</th></tr>
        </thead>
        <tbody>
            @foreach (var tipoDeAssociacao in Model)
                {
                <tr><td>@Html.ActionLink(@tipoDeAssociacao.Nome, "Details", new { tipoDeAssociacao.Id }) </td></tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Não foi dessa vez!!!</p>
}
